{
    "name": "donquixote/quick-attributes-parser",
    "description": "Quick Attributes Parser",
    "license": "GPL-2.0-or-later",
    "require-dev": {
        "doctrine/annotations": "^1.13.2",
        "infection/infection": "^0.25.5",
        "nikic/php-parser": "^4.13.2",
        "phpbench/phpbench": "^1.2.3",
        "phpunit/php-code-coverage": "^9.2.10",
        "phpunit/phpunit": "^9.5.11",
        "symfony/yaml": "^5.4.2",
        "vimeo/psalm": "^4.16.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Donquixote\\QuickAttributes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Donquixote\\QuickAttributes\\Tests\\": "tests/src"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "phpunit/phpcov": "^8.2"
    }
}
